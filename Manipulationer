-- [[ MANIPULATOR BONUS: JERKING SYSTEM ]] --
local Players = game:GetService("Players")
local LP = Players.LocalPlayer
local RS = game:GetService("RunService")
local Backpack = LP:WaitForChild("Backpack")

-- GUI'ye Yeni B√∂l√ºm Ekleme
local ScreenGui = LP.PlayerGui:FindFirstChild("Manipulator_Phase10") or Instance.new("ScreenGui", LP.PlayerGui)
local Main = ScreenGui:FindFirstChild("Frame") or Instance.new("Frame", ScreenGui)

-- DEƒûƒ∞≈ûKENLER
local JerkingActive = false
local TargetPart = nil

-- JERK TOOL OLU≈ûTURMA
local JerkTool = Instance.new("Tool")
JerkTool.Name = "Jerk"
JerkTool.RequiresHandle = false
JerkTool.Parent = Backpack

-- JERK KULLANILDIƒûINDA
JerkTool.Activated:Connect(function()
    JerkingActive = not JerkingActive
    print(JerkingActive and "Jerking Ba≈üladƒ±" or "Jerking Durdu")
end)

-- √ñZEL PAR√áA KONTROL√ú VE ANƒ∞MASYON
RS.Stepped:Connect(function()
    if not LP.Character or not LP.Character:FindFirstChild("HumanoidRootPart") then return end
    
    -- K√º√ß√ºk bir unanchored par√ßa bul ve bacak arasƒ±na sabitle
    if not TargetPart or not TargetPart.Parent then
        for _, v in pairs(workspace:GetDescendants()) do
            if v:IsA("BasePart") and not v.Anchored and v.Size.Magnitude < 10 and not v:IsDescendantOf(LP.Character) then
                TargetPart = v
                break
            end
        end
    end

    if TargetPart and TargetPart.Parent then
        local cf = LP.Character.HumanoidRootPart.CFrame
        local animOffset = Vector3.new(0, 0, 0)
        
        if JerkingActive then
            -- ƒ∞leri-Geri Hareket Animasyonu (Z ekseninde)
            local speed = 15
            local distance = 1.2
            animOffset = Vector3.new(0, -1, -1 + math.sin(tick() * speed) * distance)
            
            -- Karakter kollarƒ± i√ßin ufak bir g√∂rsel hareket (Opsiyonel)
            LP.Character.Animate.idle.Animation1.AnimationId = "rbxassetid://180435533" -- √ñrnek hareketli ID
        else
            -- Sabit duru≈ü (Bacak arasƒ±)
            animOffset = Vector3.new(0, -1, -1)
        end
        
        TargetPart.CanCollide = false
        TargetPart.Velocity = ((cf * animOffset) - TargetPart.Position) * 40
        TargetPart.CFrame = cf * CFrame.new(animOffset) * CFrame.Angles(math.rad(90), 0, 0)
    end
end)

-- GUI BUTONU
local BonusBtn = Instance.new("TextButton", Main)
BonusBtn.Size = UDim2.new(0, 220, 0, 35)
BonusBtn.Position = UDim2.new(0, 15, 0, 185) -- Phase 10 panelinin altƒ±na ekler
BonusBtn.BackgroundColor3 = Color3.fromRGB(150, 50, 0)
BonusBtn.Text = "JERKING MODE: OFF"
BonusBtn.TextColor3 = Color3.new(1,1,1)
BonusBtn.Font = Enum.Font.GothamBold
Instance.new("UICorner", BonusBtn)

BonusBtn.MouseButton1Click:Connect(function()
    if not LP.Backpack:FindFirstChild("Jerk") and not LP.Character:FindFirstChild("Jerk") then
        JerkTool.Parent = Backpack
        BonusBtn.Text = "JERKING MODE: READY"
        BonusBtn.BackgroundColor3 = Color3.fromRGB(0, 150, 0)
    else
        BonusBtn.Text = "JERK TOOL ENVANTERDE"
    end
end)

print("üîû Jerking Bonus Modu Y√ºklendi. Envanterindeki Jerk e≈üyasƒ±nƒ± kullan.")
