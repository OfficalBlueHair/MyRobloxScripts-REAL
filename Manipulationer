Title.BackgroundColor3 = Color3.fromRGB(0, 30, 30)
Title.Font = Enum.Font.Antique

-- G√ñKY√úZ√ú MERDƒ∞VENƒ∞ HESAPLAMA
local function GetStairPos(index, total)
    local t = tick()
    local p = LP.Character.HumanoidRootPart.Position
    local cf = LP.Character.HumanoidRootPart.CFrame
    
    -- Izgara (Grid) d√ºzeninde ayak altƒ±nda toplama
    local side = math.ceil(math.sqrt(total))
    local x = (index % side - side/2) * (6 * PlatformSize)
    local z = (math.floor(index / side) - side/2) * (6 * PlatformSize)
    
    -- Sen hareket ettik√ße par√ßalarƒ±n hafif√ße geriden gelmesi (Takip efekti)
    local moveOffset = LP.Character.Humanoid.MoveDirection * -5
    
    -- Ayaklarƒ±nƒ±n tam altƒ±nda, hafif√ße kavisli bir zemin
    local y = -3.5 + (math.sin(t + index*0.1) * 0.5)
    
    return cf * Vector3.new(x + moveOffset.X, y, z + moveOffset.Z)
end

-- Fƒ∞Zƒ∞K D√ñNG√úS√ú
RS.Stepped:Connect(function()
    if not Active or not LP.Character then return end
    local count = 0
    for _, v in pairs(workspace:GetDescendants()) do
        if v:IsA("BasePart") and not v.Anchored and not v:IsDescendantOf(LP.Character) then
            local dist = (v.Position - LP.Character.HumanoidRootPart.Position).Magnitude
            if dist <= Range then
                count = count + 1
                v.CanCollide = true -- BU MODDA PAR√áALARIN √úST√úNE BASABƒ∞LMEN ƒ∞√áƒ∞N COLLIDE A√áIK
                local target = GetStairPos(count, 49) -- 7x7 platform
                
                -- Par√ßalarƒ± ayak altƒ±na kilitlerken sarsƒ±ntƒ±yƒ± azaltmak i√ßin y√ºksek g√º√ß
                v.Velocity = (target - v.Position) * 50
            end
        end
    end
end)

-- KONTROLLER
local function CreateBtn(txt, pos, cb)
    local b = Instance.new("TextButton", Main)
    b.Text = txt; b.Size = UDim2.new(0, 220, 0, 35); b.Position = pos
    b.BackgroundColor3 = Color3.fromRGB(0, 80, 80); b.TextColor3 = Color3.new(1,1,1)
    b.Font = Enum.Font.GothamBold; b.MouseButton1Click:Connect(cb)
    Instance.new("UICorner", b)
end

CreateBtn("YOLU ƒ∞N≈ûA ET", UDim2.new(0, 15, 0, 50), function()
    Active = not Active
    Main.TextButton.Text = Active and "YOLU YOK ET" or "YOLU ƒ∞N≈ûA ET"
end)

