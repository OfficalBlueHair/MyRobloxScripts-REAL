-- [[ MANIPULATOR PHASE 1: THE ANATOMICAL SYMBOL ]] --
local Players = game:GetService("Players")
local LP = Players.LocalPlayer
local RS = game:GetService("RunService")

-- √ñL√úNCE Sƒ∞Lƒ∞NMEYEN GUI
local ScreenGui = Instance.new("ScreenGui", LP.PlayerGui)
ScreenGui.Name = "Manipulator_Phase1"
ScreenGui.ResetOnSpawn = false

-- DEƒûƒ∞≈ûKENLER
local Active = false
local Range = 150
local Scale = 1.5 -- Ba≈ülangƒ±√ß b√ºy√ºkl√ºƒü√º

-- ANA PANEL
local Main = Instance.new("Frame", ScreenGui)
Main.Size = UDim2.new(0, 250, 0, 200)
Main.Position = UDim2.new(0.5, -125, 0.7, 0)
Main.BackgroundColor3 = Color3.fromRGB(15, 0, 0)
Main.Active = true
Main.Draggable = true
Instance.new("UICorner", Main)

local Title = Instance.new("TextLabel", Main)
Title.Size = UDim2.new(1, 0, 0, 40)
Title.Text = "PHASE 1: THE SHAFT"
Title.TextColor3 = Color3.new(1, 1, 1)
Title.BackgroundColor3 = Color3.fromRGB(40, 0, 0)
Title.Font = Enum.Font.GothamBold

-- ANATOMƒ∞K HESAPLAMA (3 B√ñLGELƒ∞)
local function GetAnatomyPos(index, total)
    local t = tick()
    local cf = LP.Character.HumanoidRootPart.CFrame
    local s = Scale

    -- 1. B√∂lge: Testisler (Par√ßalarƒ±n %30'u)
    if index < total * 0.3 then
        local side = (index % 2 == 0) and -1 or 1
        local angle = (index / (total * 0.3)) * math.pi * 2 + t
        return cf * Vector3.new(
            (math.cos(angle) * 4 * s) + (side * 5 * s), -- Yanlara yayƒ±lma
            math.sin(angle) * 4 * s, 
            -15 * s
        )
    
    -- 2. B√∂lge: G√∂vde (Shaft) (Par√ßalarƒ±n %50'si)
    elseif index < total * 0.8 then
        local progress = (index - total * 0.3) / (total * 0.5)
        local sway = math.sin(t * 2 + progress * 5) * 0.5 * s -- Hafif sallanma
        return cf * Vector3.new(
            sway, 
            (progress * 25 * s), -- Uzunluk
            -15 * s
        )

    -- 3. B√∂lge: Ba≈ü Kƒ±sƒ±m (Tip) (Par√ßalarƒ±n %20'si)
    else
        local angle = (index / (total * 0.2)) * math.pi * 2 + t
        return cf * Vector3.new(
            math.cos(angle) * 5 * s, -- Daha geni≈ü u√ß
            (25 * s) + math.sin(angle) * 3 * s, 
            -15 * s
        )
    end
end

-- Fƒ∞Zƒ∞K MOTORU
RS.Stepped:Connect(function()
    if not Active or not LP.Character then return end
    local count = 0
    for _, v in pairs(workspace:GetDescendants()) do
        if v:IsA("BasePart") and not v.Anchored and not v:IsDescendantOf(LP.Character) then
            local dist = (v.Position - LP.Character.HumanoidRootPart.Position).Magnitude
            if dist <= Range then
                count = count + 1
                v.CanCollide = false
                local target = GetAnatomyPos(count, 60) -- 60 par√ßa hedefi
                v.Velocity = (target - v.Position) * 40
            end
        end
    end
end)

-- KONTROLLER
local function CreateBtn(txt, pos, cb)
    local b = Instance.new("TextButton", Main)
    b.Text = txt; b.Size = UDim2.new(0, 220, 0, 35); b.Position = pos
    b.BackgroundColor3 = Color3.fromRGB(60, 0, 0); b.TextColor3 = Color3.new(1,1,1)
    b.Font = Enum.Font.GothamBold; b.MouseButton1Click:Connect(cb)
    Instance.new("UICorner", b)
end

CreateBtn("AKTIF ET", UDim2.new(0, 15, 0, 50), function()
    Active = not Active
    Main.TextButton.Text = Active and "DURDUR" or "AKTIF ET"
    Main.TextButton.BackgroundColor3 = Active and Color3.fromRGB(150,0,0) or Color3.fromRGB(0,150,0)
end)

local RangeBox = Instance.new("TextBox", Main)
RangeBox.Size = UDim2.new(0, 220, 0, 35); RangeBox.Position = UDim2.new(0, 15, 0, 100)
RangeBox.BackgroundColor3 = Color3.fromRGB(30, 30, 30); RangeBox.TextColor3 = Color3.new(1,1,1)
RangeBox.Text = "MENZIL: 150"; RangeBox.Font = Enum.Font.Code
RangeBox.FocusLost:Connect(function()
    Range = tonumber(RangeBox.Text:match("%d+")) or 150
    RangeBox.Text = "MENZIL: " .. Range
end)

CreateBtn("B√úY√úKL√úK ARTIR", UDim2.new(0, 15, 0, 150), function()
    Scale = Scale + 0.5
end)

print("üèÜ Phase 1/10 Y√ºklendi. Sembol: Anatomik Shaft.")
-- [[ MANIPULATOR PHASE 2: THE OCCULT PENTAGRAM ]] --
