-- [[ MANIPULATOR PHASE 5: THE ATOMIC RINGS ]] --
local Players = game:GetService("Players")
local LP = Players.LocalPlayer
local RS = game:GetService("RunService")

-- √ñL√úNCE Sƒ∞Lƒ∞NMEYEN GUI
local ScreenGui = Instance.new("ScreenGui", LP.PlayerGui)
ScreenGui.Name = "Manipulator_Phase5"
ScreenGui.ResetOnSpawn = false

-- DEƒûƒ∞≈ûKENLER
local Active = false
local Range = 350
local Speed = 2 -- D√∂n√º≈ü hƒ±zƒ±
local Scale = 1.5 -- Y√∂r√ºnge √ßapƒ±

-- ANA PANEL
local Main = Instance.new("Frame", ScreenGui)
Main.Size = UDim2.new(0, 250, 0, 200)
Main.Position = UDim2.new(0.5, 130, 0.45, 0) -- K√ºp√ºn (Phase 4) yanƒ±na
Main.BackgroundColor3 = Color3.fromRGB(0, 40, 20)
Main.Active = true
Main.Draggable = true
Instance.new("UICorner", Main)

local Title = Instance.new("TextLabel", Main)
Title.Size = UDim2.new(1, 0, 0, 40)
Title.Text = "PHASE 5: ATOMIC RING"
Title.TextColor3 = Color3.fromRGB(0, 255, 100)
Title.BackgroundColor3 = Color3.fromRGB(0, 60, 30)
Title.Font = Enum.Font.Code

-- ATOMƒ∞K Y√ñR√úNGE HESAPLAMA
local function GetAtomicPos(index, total)
    local t = tick() * Speed
    local cf = LP.Character.HumanoidRootPart.CFrame
    local r = 12 * Scale
    
    -- Par√ßalarƒ± 3 ana y√∂r√ºngeye b√∂l√ºyoruz
    local ring = index % 3
    local angle = (index / (total / 3)) * math.pi * 2 + t
    
    local targetPos = Vector3.new(0, 0, 0)

    if ring == 0 then -- Yatay Y√∂r√ºnge (X-Z)
        targetPos = Vector3.new(math.cos(angle) * r, 2, math.sin(angle) * r)
    elseif ring == 1 then -- Dikey Y√∂r√ºnge 1 (Y-Z)
        targetPos = Vector3.new(2, math.cos(angle) * r, math.sin(angle) * r)
    elseif ring == 2 then -- Dikey Y√∂r√ºnge 2 (X-Y)
        targetPos = Vector3.new(math.cos(angle) * r, math.sin(angle) * r, 2)
    end

    -- Hafif bir yalpalama ekleyerek daha dinamik g√∂r√ºnmesini saƒülƒ±yoruz
    local wobble = CFrame.fromEulerAnglesXYZ(math.sin(t*0.5), math.cos(t*0.5), 0)
    return cf * (wobble * targetPos - Vector3.new(0, 0, 10))
end

-- Fƒ∞Zƒ∞K MOTORU
RS.Stepped:Connect(function()
    if not Active or not LP.Character then return end
    local count = 0
    for _, v in pairs(workspace:GetDescendants()) do
        if v:IsA("BasePart") and not v.Anchored and not v:IsDescendantOf(LP.Character) then
            local dist = (v.Position - LP.Character.HumanoidRootPart.Position).Magnitude
            if dist <= Range then
                count = count + 1
                v.CanCollide = false
                local target = GetAtomicPos(count, 90)
                v.Velocity = (target - v.Position) * 45
            end
        end
    end
end)

-- KONTROLLER
local function CreateBtn(txt, pos, cb)
    local b = Instance.new("TextButton", Main)
    b.Text = txt; b.Size = UDim2.new(0, 220, 0, 35); b.Position = pos
    b.BackgroundColor3 = Color3.fromRGB(0, 80, 40); b.TextColor3 = Color3.new(1,1,1)
    b.Font = Enum.Font.GothamBold; b.MouseButton1Click:Connect(cb)
    Instance.new("UICorner", b)
end

CreateBtn("ATOMU √áALI≈ûTIR", UDim2.new(0, 15, 0, 50), function()
    Active = not Active
    Main.TextButton.Text = Active and "ENERJƒ∞Yƒ∞ KES" or "ATOMU √áALI≈ûTIR"
end)

local RangeBox = Instance.new("TextBox", Main)
RangeBox.Size = UDim2.new(0, 220, 0, 35); RangeBox.Position = UDim2.new(0, 15, 0, 100)
RangeBox.BackgroundColor3 = Color3.fromRGB(10, 20, 10); RangeBox.TextColor3 = Color3.new(0,1,0)
RangeBox.Text = "MENZƒ∞L: 350"; RangeBox.Font = Enum.Font.Code
RangeBox.FocusLost:Connect(function()
    Range = tonumber(RangeBox.Text:match("%d+")) or 350
    RangeBox.Text = "MENZƒ∞L: " .. Range
end)

CreateBtn("Y√ñR√úNGEYƒ∞ GENƒ∞≈ûLET", UDim2.new(0, 15, 0, 150), function()
    Scale = Scale + 0.5
    if Scale > 5 then Scale = 1 end
end)

print("‚öõÔ∏è Phase 5/10 Y√ºklendi. Sembol: Atomic Rings.")
