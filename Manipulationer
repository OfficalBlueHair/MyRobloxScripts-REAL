-- DEƒûƒ∞≈ûKENLER
local Active = false
local Range = 500
local SuctionPower = 50 -- √áekim g√ºc√º
local ChaosLevel = 1 -- Kaos √ßarpanƒ±

-- ANA PANEL
local Main = Instance.new("Frame", ScreenGui)
Main.Size = UDim2.new(0, 250, 0, 200)
Main.Position = UDim2.new(0.5, -125, 0.2, 0) -- Ekranƒ±n en √ºst orta kƒ±smƒ±na
Main.BackgroundColor3 = Color3.fromRGB(20, 20, 25)
Main.Active = true
Main.Draggable = true
Instance.new("UICorner", Main)

local Title = Instance.new("TextLabel", Main)
Title.Size = UDim2.new(1, 0, 0, 40)
Title.Text = "PHASE 7: BLACK HOLE"
Title.TextColor3 = Color3.fromRGB(150, 0, 255)
Title.BackgroundColor3 = Color3.fromRGB(10, 10, 15)
Title.Font = Enum.Font.SpecialElite

-- KARA DELƒ∞K HESAPLAMA (VORTEX)
local function GetBlackHolePos(index, total)
    local t = tick() * 10
    local p = LP.Character.HumanoidRootPart.Position
    local cf = LP.Character.HumanoidRootPart.CFrame
    
    -- Par√ßalar merkeze ne kadar yakƒ±nsa o kadar hƒ±zlƒ± d√∂ner
    local radius = (index / total) * (5 * ChaosLevel)
    local angle = t + (index * 0.5)
    
    -- Rastgele sarsƒ±ntƒ± (Glitch efekti)
    local shake = Vector3.new(
        math.random(-1, 1) * 0.5,
        math.random(-1, 1) * 0.5,
        math.random(-1, 1) * 0.5
    )
    
    local x = math.cos(angle) * radius
    local y = math.sin(t * 0.5) * 2
    local z = math.sin(angle) * radius
    
    return p + Vector3.new(x, y, z) + shake
end

-- Fƒ∞Zƒ∞K D√ñNG√úS√ú
RS.Stepped:Connect(function()
    if not Active or not LP.Character then return end
    local count = 0
    for _, v in pairs(workspace:GetDescendants()) do
        if v:IsA("BasePart") and not v.Anchored and not v:IsDescendantOf(LP.Character) then
            local dist = (v.Position - LP.Character.HumanoidRootPart.Position).Magnitude
            if dist <= Range then
                count = count + 1
                v.CanCollide = false
                local target = GetBlackHolePos(count, 150) -- 150 par√ßa kapasitesi
                -- Kara delik √ßekim kuvveti (Hƒ±z vekt√∂r√º)
                v.Velocity = (target - v.Position) * SuctionPower
            end
        end
    end
end)

-- KONTROLLER
local function CreateBtn(txt, pos, cb)
    local b = Instance.new("TextButton", Main)
    b.Text = txt; b.Size = UDim2.new(0, 220, 0, 35); b.Position = pos
    b.BackgroundColor3 = Color3.fromRGB(40, 0, 60); b.TextColor3 = Color3.new(1,1,1)
    b.Font = Enum.Font.GothamBold; b.MouseButton1Click:Connect(cb)
    Instance.new("UICorner", b)
end

CreateBtn("OLAY UFKUNU A√á", UDim2.new(0, 15, 0, 50), function()
    Active = not Active
    Main.TextButton.Text = Active and "KARA DELƒ∞ƒûƒ∞ KAPAT" or "OLAY UFKUNU A√á"
end)

local RangeBox = Instance.new("TextBox", Main)
RangeBox.Size = UDim2.new(0, 220, 0, 35); RangeBox.Position = UDim2.new(0, 15, 0, 100)
RangeBox.BackgroundColor3 = Color3.fromRGB(10, 0, 20); RangeBox.TextColor3 = Color3.new(0.6, 0, 1)
RangeBox.Text = "MENZƒ∞L: 500"; RangeBox.Font = Enum.Font.Code
RangeBox.FocusLost:Connect(function()
    Range = tonumber(RangeBox.Text:match("%d+")) or 500
    RangeBox.Text = "MENZƒ∞L: " .. Range
end)

