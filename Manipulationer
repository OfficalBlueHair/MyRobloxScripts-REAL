-- DEƒûƒ∞≈ûKENLER
local Active = false
local Range = 1000 -- Maksimum Menzil
local AuraSize = 2 -- Aura b√ºy√ºkl√ºƒü√º
local RotationSpeed = 3 -- D√∂n√º≈ü hƒ±zƒ±

-- ANA PANEL
local Main = Instance.new("Frame", ScreenGui)
Main.Size = UDim2.new(0, 250, 0, 220)
Main.Position = UDim2.new(0.5, 130, 0.7, 0) -- Phase 2'nin yanƒ±na
