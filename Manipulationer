local LegLength = 1.5 -- Bacak uzunluƒüu √ßarpanƒ±
local Spread = 1 -- Yayƒ±lma geni≈üliƒüi

-- ANA PANEL
local Main = Instance.new("Frame", ScreenGui)
Main.Size = UDim2.new(0, 250, 0, 200)
Main.Position = UDim2.new(0.5, 130, 0.2, 0) -- Kara deliƒüin (Phase 7) yanƒ±na
