ScreenGui.Name = "Manipulator_Phase2"
ScreenGui.ResetOnSpawn = false

-- DEƒûƒ∞≈ûKENLER
local Active = false
local Range = 200
local Scale = 2.0 -- Yƒ±ldƒ±zƒ±n geni≈üliƒüi

-- ANA PANEL
local Main = Instance.new("Frame", ScreenGui)
Main.Size = UDim2.new(0, 250, 0, 200)
Main.Position = UDim2.new(0.5, 130, 0.7, 0) -- Phase 1'in yanƒ±na konumlandƒ±rdƒ±m
Main.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Main.Active = true
Main.Draggable = true
Instance.new("UICorner", Main)

local Title = Instance.new("TextLabel", Main)
Title.Size = UDim2.new(1, 0, 0, 40)
Title.Text = "PHASE 2: PENTAGRAM"
Title.TextColor3 = Color3.fromRGB(255, 0, 0)
Title.BackgroundColor3 = Color3.fromRGB(30, 0, 0)
Title.Font = Enum.Font.SpecialElite

-- PENTAGRAM HESAPLAMA (Yƒ±ldƒ±z + √áember)
local function GetPentagramPos(index, total)
    local t = tick()
    local cf = LP.Character.HumanoidRootPart.CFrame
    local s = Scale * 10

    -- 1. B√∂lge: Dƒ±≈ü √áember (Par√ßalarƒ±n %40'ƒ±)
    if index < total * 0.4 then
        local angle = (index / (total * 0.4)) * math.pi * 2 + t
        return cf * Vector3.new(math.cos(angle) * s * 1.2, math.sin(angle) * s * 1.2, -20)
    
    -- 2. B√∂lge: ƒ∞√ß Yƒ±ldƒ±z (Par√ßalarƒ±n %60'ƒ±)
    else
        -- Yƒ±ldƒ±zƒ±n 5 k√∂≈üesi arasƒ±ndaki √ßizgileri olu≈üturmak i√ßin mod√ºlo aritmetiƒüi
        local starIndex = index - (total * 0.4)
        local starTotal = total * 0.6
        local step = (starIndex / starTotal) * 5
        local p1 = math.floor(step)
        local p2 = (p1 + 2) % 5 -- Yƒ±ldƒ±z √ßizim sƒ±rasƒ± (0 -> 2 -> 4 -> 1 -> 3)
        
        local function getPoint(p)
            local a = (p / 5) * math.pi * 2 - math.pi/2
            return Vector3.new(math.cos(a) * s, math.sin(a) * s, -20)
        end
        
        local pos1 = getPoint(p1)
        local pos2 = getPoint(p2)
        local lerpVal = step - p1
        
        return cf * pos1:Lerp(pos2, lerpVal)
    end
end

-- Fƒ∞Zƒ∞K D√ñNG√úS√ú
RS.Stepped:Connect(function()
    if not Active or not LP.Character then return end
    local count = 0
    for _, v in pairs(workspace:GetDescendants()) do
        if v:IsA("BasePart") and not v.Anchored and not v:IsDescendantOf(LP.Character) then
            local dist = (v.Position - LP.Character.HumanoidRootPart.Position).Magnitude
            if dist <= Range then
                count = count + 1
                v.CanCollide = false
                local target = GetPentagramPos(count, 80) -- 80 par√ßa hedefi
                v.Velocity = (target - v.Position) * 45
            end
        end
    end
end)

-- KONTROLLER
local function CreateBtn(txt, pos, cb)
    local b = Instance.new("TextButton", Main)
    b.Text = txt; b.Size = UDim2.new(0, 220, 0, 35); b.Position = pos
    b.BackgroundColor3 = Color3.fromRGB(50, 0, 0); b.TextColor3 = Color3.new(1,1,1)
    b.Font = Enum.Font.GothamBold; b.MouseButton1Click:Connect(cb)
    Instance.new("UICorner", b)
end

CreateBtn("PENTAGRAM AKTIF", UDim2.new(0, 15, 0, 50), function()
    Active = not Active
    Main.TextButton.Text = Active and "DURDUR" or "AKTIF ET"
end)

local RangeBox = Instance.new("TextBox", Main)
RangeBox.Size = UDim2.new(0, 220, 0, 35); RangeBox.Position = UDim2.new(0, 15, 0, 100)
RangeBox.BackgroundColor3 = Color3.fromRGB(20, 20, 20); RangeBox.TextColor3 = Color3.new(1,0,0)
RangeBox.Text = "MENZIL: 200"; RangeBox.Font = Enum.Font.Code
RangeBox.FocusLost:Connect(function()
    Range = tonumber(RangeBox.Text:match("%d+")) or 200
    RangeBox.Text = "MENZIL: " .. Range
end)

CreateBtn("BOYUTU AYARLA", UDim2.new(0, 15, 0, 150), function()
    Scale = Scale + 0.5
    if Scale > 5 then Scale = 1 end
end)

print("üë∫ Phase 2/10 Y√ºklendi. Sembol: Occult Pentagram.")
