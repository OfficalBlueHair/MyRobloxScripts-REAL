Title.Font = Enum.Font.Antique

-- √ñR√úMCEK BACAK HESAPLAMA (4 BACAKLI Sƒ∞STEM)
local function GetSpiderLegPos(index, total)
    local t = tick()
    local cf = LP.Character.HumanoidRootPart.CFrame
    local rootPos = LP.Character.HumanoidRootPart.Position
    
    -- Par√ßalarƒ± 4 bacaƒüa ayƒ±rƒ±yoruz
    local legNum = index % 4
    local progress = (index / total) * 4 -- Her bacak i√ßin 0 ile 1 arasƒ± ilerleme
    
    -- Bacak a√ßƒ±larƒ± (√áapraz √ßƒ±kƒ±≈ülar)
    local angles = {
        math.rad(45),   -- Saƒü √ñn
        math.rad(135),  -- Sol √ñn
        math.rad(225),  -- Sol Arka
        math.rad(-45)   -- Saƒü Arka
    }
    
    local angle = angles[legNum + 1]
    local s = 15 * LegLength
    
    -- Bacak Kavis Form√ºl√º (Eklemli g√∂r√ºn√ºm i√ßin Parabol)
    local x = math.cos(angle) * (progress * s * Spread)
    local z = math.sin(angle) * (progress * s * Spread)
    local y = (math.sin(progress * math.pi) * 8) - (progress * 12) -- Yere doƒüru kavis
    
    -- Y√ºr√ºme sim√ºlasyonu (Hafif hareket)
    local walkCycle = math.sin(t * 5 + legNum * math.pi) * 2
    
    return cf * Vector3.new(x, y + walkCycle, z)
end

-- Fƒ∞Zƒ∞K D√ñNG√úS√ú
RS.Stepped:Connect(function()
    if not Active or not LP.Character then return end
    local count = 0
    for _, v in pairs(workspace:GetDescendants()) do
        if v:IsA("BasePart") and not v.Anchored and not v:IsDescendantOf(LP.Character) then
            local dist = (v.Position - LP.Character.HumanoidRootPart.Position).Magnitude
            if dist <= Range then
                count = count + 1
                v.CanCollide = false
                local target = GetSpiderLegPos(count, 80) -- 80 par√ßa (Bacak ba≈üƒ±na 20 par√ßa)
                v.Velocity = (target - v.Position) * 40
            end
        end
    end
end)

-- KONTROLLER
local function CreateBtn(txt, pos, cb)
    local b = Instance.new("TextButton", Main)
    b.Text = txt; b.Size = UDim2.new(0, 220, 0, 35); b.Position = pos
    b.BackgroundColor3 = Color3.fromRGB(40, 40, 40); b.TextColor3 = Color3.new(1,1,1)
    b.Font = Enum.Font.GothamBold; b.MouseButton1Click:Connect(cb)
    Instance.new("UICorner", b)
end

CreateBtn("√ñR√úMCEƒûƒ∞ UYANDIR", UDim2.new(0, 15, 0, 50), function()
    Active = not Active
    Main.TextButton.Text = Active and "BACAKLARI √áEK" or "√ñR√úMCEƒûƒ∞ UYANDIR"
end)

local RangeBox = Instance.new("TextBox", Main)
RangeBox.Size = UDim2.new(0, 220, 0, 35); RangeBox.Position = UDim2.new(0, 15, 0, 100)
