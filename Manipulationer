-- DEƒûƒ∞≈ûKENLER
local Active = false
local Range = 400
local Width = 5 -- Geni≈ülik √ßarpanƒ±
local Height = 4 -- Y√ºkseklik √ßarpanƒ±

-- ANA PANEL
local Main = Instance.new("Frame", ScreenGui)
Main.Size = UDim2.new(0, 250, 0, 200)
Main.Position = UDim2.new(0.5, -385, 0.45, 0) -- DNA Panelinin √ºst√ºne
Main.BackgroundColor3 = Color3.fromRGB(50, 50, 0)
Main.Active = true
Main.Draggable = true
Instance.new("UICorner", Main)

local Title = Instance.new("TextLabel", Main)
Title.Size = UDim2.new(1, 0, 0, 40)
Title.Text = "PHASE 6: SHIELD WALL"
Title.TextColor3 = Color3.fromRGB(255, 255, 0)
Title.BackgroundColor3 = Color3.fromRGB(70, 70, 0)
Title.Font = Enum.Font.GothamBold

-- KALKAN DUVARI HESAPLAMA
local function GetShieldPos(index, total)
    local t = tick()
    local cf = LP.Character.HumanoidRootPart.CFrame
    
    -- Matris dizilimi (√ñrn: 10x10)
    local columns = 8
    local col = index % columns
    local row = math.floor(index / columns)
    
    -- Kavisli (curved) efekt vermek i√ßin sin√ºs kullanƒ±yoruz
    local x = (col - columns/2) * (4 * Width)
    local y = (row) * (4 * Height) - 5
    local z = -15 - (math.abs(col - columns/2) * 2) -- Kenarlar daha geride durur (kavis)
    
    -- Duvarƒ±n hafif√ße nefes alƒ±yormu≈ü gibi √∂ne arkaya gitmesi
    local breathe = math.sin(t * 2) * 2
    
    return cf * Vector3.new(x, y, z + breathe)
end

-- Fƒ∞Zƒ∞K D√ñNG√úS√ú
RS.Stepped:Connect(function()
    if not Active or not LP.Character then return end
    local count = 0
    for _, v in pairs(workspace:GetDescendants()) do
        if v:IsA("BasePart") and not v.Anchored and not v:IsDescendantOf(LP.Character) then
            local dist = (v.Position - LP.Character.HumanoidRootPart.Position).Magnitude
            if dist <= Range then
                count = count + 1
                v.CanCollide = false
                local target = GetShieldPos(count, 64) -- 64 par√ßa (8x8 duvar)
                v.Velocity = (target - v.Position) * 40
            end
        end
    end
end)

-- KONTROLLER
local function CreateBtn(txt, pos, cb)
    local b = Instance.new("TextButton", Main)
    b.Text = txt; b.Size = UDim2.new(0, 220, 0, 35); b.Position = pos
    b.BackgroundColor3 = Color3.fromRGB(90, 90, 0); b.TextColor3 = Color3.new(1,1,1)
    b.Font = Enum.Font.GothamBold; b.MouseButton1Click:Connect(cb)
    Instance.new("UICorner", b)
end

CreateBtn("KALKANI AKTƒ∞FLE", UDim2.new(0, 15, 0, 50), function()
    Active = not Active
    Main.TextButton.Text = Active and "KALKANI ƒ∞NDƒ∞R" or "KALKANI AKTƒ∞FLE"
end)

local RangeBox = Instance.new("TextBox", Main)
RangeBox.Size = UDim2.new(0, 220, 0, 35); RangeBox.Position = UDim2.new(0, 15, 0, 100)
RangeBox.BackgroundColor3 = Color3.fromRGB(30, 30, 0); RangeBox.TextColor3 = Color3.new(1,1,1)
RangeBox.Text = "MENZƒ∞L: 400"; RangeBox.Font = Enum.Font.Code
RangeBox.FocusLost:Connect(function()
    Range = tonumber(RangeBox.Text:match("%d+")) or 400
    RangeBox.Text = "MENZƒ∞L: " .. Range
end)

CreateBtn("KALKANI GENƒ∞≈ûLET", UDim2.new(0, 15, 0, 150), function()
    Width = Width + 1
    Height = Height + 0.5
    if Width > 15 then Width = 5 Height = 4 end
end)

