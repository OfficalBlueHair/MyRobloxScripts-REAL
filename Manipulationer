    local b = Instance.new("TextButton", Main)
    b.Text = txt; b.Size = UDim2.new(0, 220, 0, 35); b.Position = pos
    b.BackgroundColor3 = Color3.fromRGB(70, 0, 70); b.TextColor3 = Color3.new(1,1,1)
    b.Font = Enum.Font.GothamBold; b.MouseButton1Click:Connect(cb)
    Instance.new("UICorner", b)
end

CreateBtn("ƒ∞LAHƒ∞ G√úC√ú A√á", UDim2.new(0, 15, 0, 50), function()
    Active = not Active
    Main.TextButton.Text = Active and "G√úC√ú M√úH√úRLE" or "ƒ∞LAHƒ∞ G√úC√ú A√á"
end)

local RangeBox = Instance.new("TextBox", Main)
RangeBox.Size = UDim2.new(0, 220, 0, 35); RangeBox.Position = UDim2.new(0, 15, 0, 100)
RangeBox.BackgroundColor3 = Color3.fromRGB(20, 0, 20); RangeBox.TextColor3 = Color3.new(1, 0.8, 0)
RangeBox.Text = "MENZƒ∞L: 1000"; RangeBox.Font = Enum.Font.Code
