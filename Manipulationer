Main.Draggable = true
Instance.new("UICorner", Main)

local Title = Instance.new("TextLabel", Main)
Title.Size = UDim2.new(1, 0, 0, 40)
Title.Text = "FINAL PHASE: GOD MODE"
Title.TextColor3 = Color3.fromRGB(255, 215, 0) -- Altƒ±n Sarƒ±sƒ±
Title.BackgroundColor3 = Color3.fromRGB(50, 0, 50)
Title.Font = Enum.Font.Antique

-- TANRI MODU HESAPLAMA (TA√á + AURA)
local function GetGodModePos(index, total)
    local t = tick() * RotationSpeed
    local cf = LP.Character.HumanoidRootPart.CFrame
    local headPos = LP.Character.Head.Position
    
    -- 1. B√∂lge: Tanrƒ± Tacƒ± (Par√ßalarƒ±n %20'si)
    if index < total * 0.2 then
        local angle = (index / (total * 0.2)) * math.pi * 2 + t
        local radius = 5 * AuraSize
        return cf * Vector3.new(math.cos(angle) * radius, 8 + math.sin(t*2), math.sin(angle) * radius)
    
    -- 2. B√∂lge: Devasa Arka Aura (Par√ßalarƒ±n %80'i)
    else
        local auraIndex = index - (total * 0.2)
        local auraTotal = total * 0.8
        local angle = (auraIndex / auraTotal) * math.pi * 2
        local dist = 15 * AuraSize
        
        -- Kanat benzeri dalgalanma efekti
        local wave = math.sin(t + (auraIndex * 0.1)) * 5
        local x = math.cos(angle) * dist
        local y = math.sin(angle) * dist + 10
        local z = 10 + wave -- Arkada durmasƒ± i√ßin
        
        return cf * Vector3.new(x, y, z)
    end
end

-- Fƒ∞Zƒ∞K D√ñNG√úS√ú
RS.Stepped:Connect(function()
    if not Active or not LP.Character then return end
    local count = 0
    for _, v in pairs(workspace:GetDescendants()) do
        if v:IsA("BasePart") and not v.Anchored and not v:IsDescendantOf(LP.Character) then
            local dist = (v.Position - LP.Character.HumanoidRootPart.Position).Magnitude
            if dist <= Range then
                count = count + 1
                v.CanCollide = false
                local target = GetGodModePos(count, 150) -- 150 par√ßa ile devasa g√∂r√ºn√ºm
                v.Velocity = (target - v.Position) * 45
            end
        end
    end
end)

-- KONTROLLER
