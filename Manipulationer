ScreenGui.ResetOnSpawn = false

-- DEƒûƒ∞≈ûKENLER
local Active = false
local Range = 250
local Scale = 1.5 -- Geni≈ülik
local HeightScale = 2 -- Y√ºkseklik √ßarpanƒ±

-- ANA PANEL
local Main = Instance.new("Frame", ScreenGui)
Main.Size = UDim2.new(0, 250, 0, 200)
Main.Position = UDim2.new(0.5, -385, 0.7, 0) -- Phase 1 ve 2'nin soluna konumlandƒ±rdƒ±m
