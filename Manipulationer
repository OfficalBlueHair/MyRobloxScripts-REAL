Title.Size = UDim2.new(1, 0, 0, 40)
Title.Text = "PHASE 3: DNA HELIX"
Title.TextColor3 = Color3.fromRGB(0, 255, 255)
Title.BackgroundColor3 = Color3.fromRGB(0, 40, 80)
Title.Font = Enum.Font.Code

-- DNA HELIX HESAPLAMA (√áift Sarmallƒ± 3D Yapƒ±)
local function GetDNAPos(index, total)
    local t = tick() * 2 -- D√∂n√º≈ü hƒ±zƒ±
    local cf = LP.Character.HumanoidRootPart.CFrame
    local radius = 8 * Scale
    local verticalSpacing = 1.2 * HeightScale
    
    -- Par√ßalarƒ± iki gruba ayƒ±rƒ±yoruz (Sol sarmal ve Saƒü sarmal)
    local side = (index % 2 == 0) and 0 or math.pi -- 180 derece fark
    local progress = index / 2
    local angle = (progress * 0.5) + t + side
    
    local x = math.cos(angle) * radius
    local z = math.sin(angle) * radius
    local y = progress * verticalSpacing - (total * 0.25) -- Ortalamak i√ßin
    
    return cf * Vector3.new(x, y, z - 20)
end

-- Fƒ∞Zƒ∞K MOTORU
RS.Stepped:Connect(function()
    if not Active or not LP.Character then return end
    local count = 0
    for _, v in pairs(workspace:GetDescendants()) do
        if v:IsA("BasePart") and not v.Anchored and not v:IsDescendantOf(LP.Character) then
            local dist = (v.Position - LP.Character.HumanoidRootPart.Position).Magnitude
            if dist <= Range then
                count = count + 1
                v.CanCollide = false
                local target = GetDNAPos(count, 100) -- Daha fazla par√ßa kapasitesi
                v.Velocity = (target - v.Position) * 35
            end
        end
    end
end)

-- KONTROLLER
local function CreateBtn(txt, pos, cb)
    local b = Instance.new("TextButton", Main)
    b.Text = txt; b.Size = UDim2.new(0, 220, 0, 35); b.Position = pos
    b.BackgroundColor3 = Color3.fromRGB(0, 60, 100); b.TextColor3 = Color3.new(1,1,1)
    b.Font = Enum.Font.GothamBold; b.MouseButton1Click:Connect(cb)
    Instance.new("UICorner", b)
end

CreateBtn("DNA AKTIF", UDim2.new(0, 15, 0, 50), function()
    Active = not Active
    Main.TextButton.Text = Active and "DURDUR" or "AKTIF ET"
end)

local RangeBox = Instance.new("TextBox", Main)
RangeBox.Size = UDim2.new(0, 220, 0, 35); RangeBox.Position = UDim2.new(0, 15, 0, 100)
RangeBox.BackgroundColor3 = Color3.fromRGB(10, 30, 50); RangeBox.TextColor3 = Color3.new(0,1,1)
RangeBox.Text = "MENZIL: 250"; RangeBox.Font = Enum.Font.Code
