local RS = game:GetService("RunService")

-- √ñL√úNCE Sƒ∞Lƒ∞NMEYEN GUI
local ScreenGui = Instance.new("ScreenGui", LP.PlayerGui)
ScreenGui.Name = "Manipulator_Phase4"
ScreenGui.ResetOnSpawn = false

-- DEƒûƒ∞≈ûKENLER
local Active = false
local Range = 300
local Size = 1.5 -- K√ºp√ºn b√ºy√ºkl√ºƒü√º

-- ANA PANEL
local Main = Instance.new("Frame", ScreenGui)
Main.Size = UDim2.new(0, 250, 0, 200)
Main.Position = UDim2.new(0.5, -125, 0.45, 0) -- Ekranƒ±n √ºst-orta kƒ±smƒ±na
Main.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
Main.Active = true
Main.Draggable = true
Instance.new("UICorner", Main)

local Title = Instance.new("TextLabel", Main)
Title.Size = UDim2.new(1, 0, 0, 40)
Title.Text = "PHASE 4: 3D CUBE"
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
Title.Font = Enum.Font.GothamBold

-- K√úP Y√úZEY HESAPLAMA
local function GetCubePos(index, total)
    local t = tick()
    local cf = LP.Character.HumanoidRootPart.CFrame
    local s = 10 * Size
    
    -- Par√ßalarƒ± 6 y√ºzeye ayƒ±r (Her y√ºzey i√ßin e≈üit par√ßa)
    local face = index % 6
    local step = (index / total) * 2 - 1 -- -1 ile 1 arasƒ± deƒüer
    local offset1 = math.sin(step * math.pi) * s
    local offset2 = math.cos(step * math.pi) * s

    -- Rotasyon matrisi (K√ºp√ºn kendi ekseninde d√∂nmesi i√ßin)
    local rot = CFrame.fromEulerAnglesXYZ(t, t/2, 0)
    local targetPos = Vector3.new(0, 0, 0)

    if face == 0 then targetPos = Vector3.new(s, offset1, offset2)     -- Saƒü
    elseif face == 1 then targetPos = Vector3.new(-s, offset1, offset2)    -- Sol
    elseif face == 2 then targetPos = Vector3.new(offset1, s, offset2)     -- √úst
    elseif face == 3 then targetPos = Vector3.new(offset1, -s, offset2)    -- Alt
    elseif face == 4 then targetPos = Vector3.new(offset1, offset2, s)     -- √ñn
    elseif face == 5 then targetPos = Vector3.new(offset1, offset2, -s)    -- Arka
    end

    return cf * (rot * targetPos - Vector3.new(0, 0, 15))
end

-- Fƒ∞Zƒ∞K MOTORU
RS.Stepped:Connect(function()
    if not Active or not LP.Character then return end
    local count = 0
    for _, v in pairs(workspace:GetDescendants()) do
        if v:IsA("BasePart") and not v.Anchored and not v:IsDescendantOf(LP.Character) then
            local dist = (v.Position - LP.Character.HumanoidRootPart.Position).Magnitude
            if dist <= Range then
                count = count + 1
                v.CanCollide = false
                local target = GetCubePos(count, 120) -- 120 par√ßa sƒ±nƒ±rƒ±
                v.Velocity = (target - v.Position) * 40
            end
        end
    end
end)

-- KONTROLLER
local function CreateBtn(txt, pos, cb)
    local b = Instance.new("TextButton", Main)
    b.Text = txt; b.Size = UDim2.new(0, 220, 0, 35); b.Position = pos
    b.BackgroundColor3 = Color3.fromRGB(80, 80, 80); b.TextColor3 = Color3.new(1,1,1)
    b.Font = Enum.Font.GothamBold; b.MouseButton1Click:Connect(cb)
    Instance.new("UICorner", b)
end

CreateBtn("K√úP√ú AKTƒ∞FLE≈ûTƒ∞R", UDim2.new(0, 15, 0, 50), function()
    Active = not Active
    Main.TextButton.Text = Active and "K√úP√ú DURDUR" or "K√úP√ú AKTƒ∞FLE≈ûTƒ∞R"
end)

local RangeBox = Instance.new("TextBox", Main)
RangeBox.Size = UDim2.new(0, 220, 0, 35); RangeBox.Position = UDim2.new(0, 15, 0, 100)
RangeBox.BackgroundColor3 = Color3.fromRGB(20, 20, 20); RangeBox.TextColor3 = Color3.new(1,1,1)
RangeBox.Text = "MENZƒ∞L: 300"; RangeBox.Font = Enum.Font.Code
